{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "person_id": {"type": "string"},
        "sex": {"type": "string"},
        "education_level": {"type": "string"},
        "group_id": {"type": "string", "enum": ["PREG", "WRA", "PSC", "SAC", "MEN"]},
        "age_in_months": {"type": "integer"},
        "is_breastfed": {"type": "boolean"},
        "is_lactating": {"type": "boolean"},
        "is_pregnant": {"type": "boolean"},
        "is_smoker": {"type": "boolean"},
        "had_illness": {"type": "boolean"},
        "had_malaria": {"type": "boolean"},
        "had_diarrhoea": {"type": "boolean"},
        "survey_cluster": {"type": "string"},
        "survey_strata": {"type": "integer"},
        "survey_weight": {"type": "integer"},
        "literacy": {"type": "string"},
        "weight_in_kg": {"type": "number"},
        "height_in_cm": {"type": "number"},
        "bmi": {"type": "number"},
        "interview_date": {"type": "string", "format": "date"},
        "date_sampled": {"type": "string", "format": "date"},
        "sample_type": {"type": "string"},
        "haemoglobin": {"type": "number"},
        "ferritin": {"type": "number"},
        "stfr": {"type": "number"},
        "rbp": {"type": "number"},
        "retinol": {"type": "number"},
        "rbc_folate": {"type": "number"},
        "ps_folate": {"type": "number"},
        "vitamin_b12": {"type": "number"},
        "zinc": {"type": "number"},
        "crp": {"type": "number"},
        "agp": {"type": "number"},
        "iodine": {"type": "number"},
        "time_of_day_sampled": {"type": "string", "enum": ["am", "pm"]},
        "was_fasting": {"type": "boolean"}
      },
    //   "required": ["person_id", "sex", "group_id", "age_in_months", "survey_cluster", "survey_strata", "survey_weight"]
    }
  }
